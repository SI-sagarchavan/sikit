{
  "name": "@acme/ui",
  "version": "2.1.0",
  "sideEffects": [
    "**/*.css"
  ],
  "license": "MIT",
  "type": "module",
  "exports": {
    "./styles.css": "./dist/styles.css",
    ".": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "unpkg": "./dist/index.global.js",
  "jsdelivr": "./dist/index.global.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup && npx @tailwindcss/cli -i ./src/styles.css -o ./dist/styles.css",
    "dev": "tsup --watch && npx @tailwindcss/cli -i ./src/styles.css -o ./dist/styles.css --watch",
    "lint": "eslint . --max-warnings 0",
    "clean": "rm -rf .turbo node_modules dist"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/figlet": "^1.7.0",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "eslint": "^8.57.0",
    "tailwindcss": "^4.1.13",
    "tsup": "^8.0.2",
    "typescript": "5.5.4"
  },
  "peerDependencies": {
    "react": "^19.1.0",
    "tailwind-variants": "^3.1.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "date-fns": "^4.1.0",
    "figlet": "^1.8.1",
    "react-day-picker": "^9.10.0",
    "zod": "^4.1.9",
    "zod-to-json-schema": "^3.24.6"
  }
}